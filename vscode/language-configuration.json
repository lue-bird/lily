{
  "comments": {
    "lineComment": {
      "comment": "#",
      "noIndent": false
    },
    "blockComment": [
      "(#",
      "#)"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ],
    [
      ":",
      ":"
    ],
  ],
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string"
      ]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": ":",
      "close": ":",
      "notIn": [
        "string",
        "comment"
      ]
    },
  ],
  "wordPattern": "[a-zA-Z][a-zA-Z\\-0-9]*",
}