{
  "fileTypes": [
    "still"
  ],
  "name": "still",
  "scopeName": "source.still",
  "patterns": [
    {
      "include": "#debug"
    },
    {
      "include": "#comments"
    },
    {
      "match": "\\b(_)\\b",
      "name": "keyword.unused.still"
    },
    {
      "include": "#type-signature"
    },
    {
      "include": "#type-declaration"
    },
    {
      "include": "#type-alias-declaration"
    },
    {
      "include": "#string-triple"
    },
    {
      "include": "#string-quote"
    },
    {
      "include": "#char"
    },
    {
      "comment": "Floats are always decimal",
      "match": "\\b([0-9]+\\.[0-9]+([eE][+-]?[0-9]+)?|[0-9]+[eE][+-]?[0-9]+)\\b",
      "name": "constant.numeric.float.still"
    },
    {
      "match": "\\b([0-9]+)\\b",
      "name": "constant.numeric.still"
    },
    {
      "match": "\\b(0x[0-9a-fA-F]+)\\b",
      "name": "constant.numeric.still"
    },
    {
      "include": "#record-prefix"
    },
    {
      "include": "#module-prefix"
    },
    {
      "include": "#constructor"
    },
    {
      "name": "meta.record.field.update.still",
      "match": "(\\{)\\s+([a-z][a-zA-Z0-9_]*)\\s+(\\|)\\s+([a-z][a-zA-Z0-9_]*)",
      "captures": {
        "1": {
          "name": "punctuation.bracket.still"
        },
        "2": {
          "name": "record.name.still"
        },
        "3": {
          "name": "keyword.pipe.still"
        },
        "4": {
          "name": "entity.name.record.field.still"
        }
      }
    },
    {
      "name": "meta.record.field.update.still",
      "match": "(\\|)\\s+([a-z][a-zA-Z0-9_]*)\\s+(\\=)",
      "captures": {
        "1": {
          "name": "keyword.pipe.still"
        },
        "2": {
          "name": "entity.name.record.field.still"
        },
        "3": {
          "name": "keyword.operator.assignment.still"
        }
      }
    },
    {
      "name": "meta.record.field.update.still",
      "match": "(\\{)\\s+([a-z][a-zA-Z0-9_]*)\\s+$",
      "captures": {
        "1": {
          "name": "punctuation.bracket.still"
        },
        "2": {
          "name": "record.name.still"
        }
      }
    },
    {
      "name": "meta.record.field.still",
      "match": "(\\{)\\s+([a-z][a-zA-Z0-9_]*)\\s+(\\=)",
      "captures": {
        "1": {
          "name": "punctuation.bracket.still"
        },
        "2": {
          "name": "entity.name.record.field.still"
        },
        "3": {
          "name": "keyword.operator.assignment.still"
        }
      }
    },
    {
      "name": "meta.record.field.still",
      "match": "(,)\\s+([a-z][a-zA-Z0-9_]*)\\s+(\\=)",
      "captures": {
        "1": {
          "name": "punctuation.separator.comma.still"
        },
        "2": {
          "name": "entity.name.record.field.still"
        },
        "3": {
          "name": "keyword.operator.assignment.still"
        }
      }
    },
    {
      "match": "(\\}|\\{)",
      "name": "punctuation.bracket.still"
    },
    {
      "include": "#unit"
    },
    {
      "include": "#comma"
    },
    {
      "include": "#parens"
    },
    {
      "match": "(->)",
      "name": "keyword.operator.arrow.still"
    },
    {
      "include": "#infix_op"
    },
    {
      "match": "(\\=|\\:|\\||\\\\)",
      "name": "keyword.other.still"
    },
    {
      "match": "\\b(type|as|port|exposing|alias|infixl|infixr|infix)\\s+",
      "name": "keyword.other.still"
    },
    {
      "match": "\\b(if|then|else|case|of|let|in)\\s+",
      "name": "keyword.control.still"
    },
    {
      "include": "#record-accessor"
    },
    {
      "include": "#top_level_value"
    },
    {
      "include": "#value"
    },
    {
      "include": "#period"
    },
    {
      "include": "#square_brackets"
    }
  ],
  "repository": {
    "comma": {
      "match": "(,)",
      "name": "punctuation.separator.comma.still"
    },
    "parens": {
      "match": "(\\(|\\))",
      "name": "punctuation.parens.still"
    },
    "block_comment": {
      "applyEndPatternLast": 1,
      "begin": "\\{-(?!#)",
      "captures": {
        "0": {
          "name": "punctuation.definition.comment.still"
        }
      },
      "end": "-\\}",
      "name": "comment.block.still",
      "patterns": [
        {
          "include": "#block_comment"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "captures": {
            "1": {
              "name": "punctuation.definition.comment.still"
            }
          },
          "begin": "--",
          "end": "$",
          "name": "comment.line.double-dash.still"
        },
        {
          "include": "#block_comment"
        }
      ]
    },
    "string-triple": {
      "name": "string.quoted.triple.still",
      "begin": "\"\"\"",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.still"
        }
      },
      "end": "\"\"\"",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.still"
        }
      },
      "patterns": [
        {
          "match": "\\\\(NUL|SOH|STX|ETX|EOT|ENQ|ACK|BEL|BS|HT|LF|VT|FF|CR|SO|SI|DLE|DC1|DC2|DC3|DC4|NAK|SYN|ETB|CAN|EM|SUB|ESC|FS|GS|RS|US|SP|DEL|[abfnrtv\\\\\\\"'\\&]|x[0-9a-fA-F]{1,5})",
          "name": "constant.character.escape.still"
        },
        {
          "match": "\\^[A-Z@\\[\\]\\\\\\^_]",
          "name": "constant.character.escape.control.still"
        }
      ]
    },
    "string-quote": {
      "name": "string.quoted.double.still",
      "begin": "\"",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.still"
        }
      },
      "end": "\"",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.still"
        }
      },
      "patterns": [
        {
          "match": "\\\\(NUL|SOH|STX|ETX|EOT|ENQ|ACK|BEL|BS|HT|LF|VT|FF|CR|SO|SI|DLE|DC1|DC2|DC3|DC4|NAK|SYN|ETB|CAN|EM|SUB|ESC|FS|GS|RS|US|SP|DEL|[abfnrtv\\\\\\\"'\\&]|x[0-9a-fA-F]{1,5})",
          "name": "constant.character.escape.still"
        },
        {
          "match": "\\^[A-Z@\\[\\]\\\\\\^_]",
          "name": "constant.character.escape.control.still"
        }
      ]
    },
    "char": {
      "name": "string.quoted.single.still",
      "begin": "'",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.char.begin.still"
        }
      },
      "end": "'",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.char.end.still"
        }
      },
      "patterns": [
        {
          "match": "\\\\(NUL|SOH|STX|ETX|EOT|ENQ|ACK|BEL|BS|HT|LF|VT|FF|CR|SO|SI|DLE|DC1|DC2|DC3|DC4|NAK|SYN|ETB|CAN|EM|SUB|ESC|FS|GS|RS|US|SP|DEL|[abfnrtv\\\\\\\"'\\&]|x[0-9a-fA-F]{1,5})",
          "name": "constant.character.escape.still"
        },
        {
          "match": "\\^[A-Z@\\[\\]\\\\\\^_]",
          "name": "constant.character.escape.control.still"
        }
      ]
    },
    "period": {
      "match": "[.]",
      "name": "keyword.other.period.still"
    },
    "square_brackets": {
      "match": "[\\[\\]]",
      "name": "punctuation.definition.list.still"
    },
    "record-prefix": {
      "match": "([a-z][a-zA-Z0-9_]*)(\\.)([a-z][a-zA-Z0-9_]*)",
      "name": "record.accessor.still",
      "captures": {
        "1": {
          "name": "record.name.still"
        },
        "2": {
          "name": "keyword.other.period.still"
        },
        "3": {
          "name": "entity.name.record.field.accessor.still"
        }
      }
    },
    "module-prefix": {
      "match": "([A-Z][a-zA-Z0-9_]*)(\\.)",
      "name": "meta.module.name.still",
      "captures": {
        "1": {
          "name": "support.module.still"
        },
        "2": {
          "name": "keyword.other.period.still"
        }
      }
    },
    "constructor": {
      "match": "\\b[A-Z][a-zA-Z0-9_]*\\b",
      "name": "constant.type-constructor.still"
    },
    "value": {
      "match": "\\b[a-z][a-zA-Z0-9_]*\\b",
      "name": "meta.value.still"
    },
    "unit": {
      "match": "\\(\\)",
      "name": "constant.unit.still"
    },
    "top_level_value": {
      "match": "^[a-z][a-zA-Z0-9_]*\\b",
      "name": "entity.name.function.top_level.still"
    },
    "record-accessor": {
      "match": "(\\.)([a-z][a-zA-Z0-9_]*)",
      "name": "meta.record.accessor",
      "captures": {
        "1": {
          "name": "keyword.other.period.still"
        },
        "2": {
          "name": "entity.name.record.field.accessor.still"
        }
      }
    },
    "infix_op": {
      "match": "(</>|<\\?>|<\\||<=|\\|\\||&&|>=|\\|>|\\|=|\\|\\.|\\+\\+|::|/=|==|//|>>|<<|<|>|\\^|\\+|-|/|\\*)",
      "name": "keyword.operator.still"
    },
    "type-declaration": {
      "begin": "^(type\\s+)([A-Z][a-zA-Z0-9_']*)\\s+",
      "beginCaptures": {
        "1": {
          "name": "keyword.type.still"
        },
        "2": {
          "name": "storage.type.still"
        }
      },
      "end": "^(?=\\S)",
      "name": "meta.function.type-declaration.still",
      "patterns": [
        {
          "name": "meta.record.field.still",
          "match": "^\\s*([A-Z][a-zA-Z0-9_]*)\\b",
          "captures": {
            "1": {
              "name": "constant.type-constructor.still"
            }
          }
        },
        {
          "match": "\\s+",
          "name": "punctuation.spaces.still"
        },
        {
          "name": "meta.record.field.still",
          "match": "(\\=|\\|)\\s+([A-Z][a-zA-Z0-9_]*)\\b",
          "captures": {
            "1": {
              "name": "keyword.operator.assignment.still"
            },
            "2": {
              "name": "constant.type-constructor.still"
            }
          }
        },
        {
          "match": "\\=",
          "name": "keyword.operator.assignment.still"
        },
        {
          "match": "\\-\\>",
          "name": "keyword.operator.arrow.still"
        },
        {
          "include": "#module-prefix"
        },
        {
          "match": "\\b[a-z][a-zA-Z0-9_]*\\b",
          "name": "variable.type.still"
        },
        {
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b",
          "name": "storage.type.still"
        },
        {
          "include": "#comments"
        },
        {
          "include": "#type-record"
        }
      ]
    },
    "type-alias-declaration": {
      "begin": "^(type\\s+)(alias\\s+)([A-Z][a-zA-Z0-9_']*)\\s+",
      "beginCaptures": {
        "1": {
          "name": "keyword.type.still"
        },
        "2": {
          "name": "keyword.type-alias.still"
        },
        "3": {
          "name": "storage.type.still"
        }
      },
      "end": "^(?=\\S)",
      "name": "meta.function.type-declaration.still",
      "patterns": [
        {
          "match": "\\n\\s+",
          "name": "punctuation.spaces.still"
        },
        {
          "match": "\\=",
          "name": "keyword.operator.assignment.still"
        },
        {
          "include": "#module-prefix"
        },
        {
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b",
          "name": "storage.type.still"
        },
        {
          "match": "\\b[a-z][a-zA-Z0-9_]*\\b",
          "name": "variable.type.still"
        },
        {
          "include": "#comments"
        },
        {
          "include": "#type-record"
        }
      ]
    },
    "type-record": {
      "begin": "(\\{)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.section.braces.begin"
        }
      },
      "end": "(\\})",
      "endCaptures": {
        "1": {
          "name": "punctuation.section.braces.end"
        }
      },
      "name": "meta.function.type-record.still",
      "patterns": [
        {
          "match": "\\s+",
          "name": "punctuation.spaces.still"
        },
        {
          "match": "->",
          "name": "keyword.operator.arrow.still"
        },
        {
          "name": "meta.record.field.still",
          "match": "([a-z][a-zA-Z0-9_]*)\\s+(\\:)",
          "captures": {
            "1": {
              "name": "entity.name.record.field.still"
            },
            "2": {
              "name": "keyword.other.still"
            }
          }
        },
        {
          "match": "\\,",
          "name": "punctuation.separator.comma.still"
        },
        {
          "include": "#module-prefix"
        },
        {
          "match": "\\b[a-z][a-zA-Z0-9_]*\\b",
          "name": "variable.type.still"
        },
        {
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b",
          "name": "storage.type.still"
        },
        {
          "include": "#comments"
        },
        {
          "include": "#type-record"
        }
      ]
    },
    "type-signature": {
      "begin": "^(port\\s+)?([a-z_][a-zA-Z0-9_']*)\\s+(\\:)",
      "beginCaptures": {
        "1": {
          "name": "keyword.other.port.still"
        },
        "2": {
          "name": "entity.name.function.still"
        },
        "3": {
          "name": "keyword.other.colon.still"
        }
      },
      "end": "((^(?=[a-z]))|^$)",
      "name": "meta.function.type-declaration.still",
      "patterns": [
        {
          "include": "#type-signature-chunk"
        }
      ]
    },
    "type-signature-chunk": {
      "patterns": [
        {
          "match": "->",
          "name": "keyword.operator.arrow.still"
        },
        {
          "match": "\\s+",
          "name": "punctuation.spaces.still"
        },
        {
          "include": "#module-prefix"
        },
        {
          "match": "\\b[a-z][a-zA-Z0-9_]*\\b",
          "name": "variable.type.still"
        },
        {
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b",
          "name": "storage.type.still"
        },
        {
          "match": "\\(\\)",
          "name": "constant.unit.still"
        },
        {
          "include": "#comma"
        },
        {
          "include": "#parens"
        },
        {
          "include": "#comments"
        },
        {
          "include": "#type-record"
        }
      ]
    }
  }
}